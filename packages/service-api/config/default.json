{
  "deployment": {
    "containerName": "rnd-keycloak-stripe-api",
    "imageName": "tuiteraz/rnd-keycloak-stripe-api"
  },
  "runtime": {
    "environment": "development",
    "logLevel": "debug",
    "port": 8010,
    "host": "http://localhost"
  },
  "ai-providers": {
    "default": "open-ai",
    "open-ai": {
      "apiKey": "***",
      "default-model": "gpt-4o-mini"
    },
    "anthropic": {
      "apiKey": "***",
      "default-model": "claude-3.5-sonnet"
    }
  }
}